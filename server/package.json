    "mcp",
    "model-context-protocol",
    "prompts",
    "language-model",
    "server"
  ],
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "@types/nunjucks": "^3.2.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "nunjucks": "^3.2.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
